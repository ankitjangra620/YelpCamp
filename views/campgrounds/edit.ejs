<%-include("../partials/header.ejs")%>
<div class="container justify-content-center">
	<h1 class="text-center mt-5">
	Edit <%=camp.title%>
</h1>
	<div style="width:30%; margin:0 auto;">
<form action="/campgrounds/<%=camp._id%>/edit/update?_method=PUT"  method="POST">
	<div class="form-group">
		<div style="margin-top:40px;">
				<input class="form-control"type="text" name="title" value="<%=camp.title%>">
		</div>
	
	</div>
	
	<div class="form-group">
		<input class="form-control"type="text" name="image" value="<%=camp.image%>">
	</div>
	
		
	<div class="form-group">
		<input class="form-control"type="text" name="description" value="<%=camp.description%>">
	</div>
	
	
	<div class="form-group">
		<input class="btn-block btn-primary"type="submit">
	</div>
	<div class="form-group">
		<button >
			Go Back
		</button>
	</div>
	
</form>
</div>
</div>


<%-include ("../partials/footer.ejs")%>